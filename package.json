{
  "name": "vite-plugin-add-importmap",
  "version": "0.1.0",
  "description": "A vite plugin adding importmap to HTML file",
  "author": "CYF",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "release:major": "standard-version -- --release-as major",
    "release:minor": "standard-version -- --release-as minor",
    "release:patch": "standard-version -- --release-as patch"
  },
  "files": [
    "dist",
    "README.md",
    "src"
  ],
  "keywords": [
    "vite",
    "importmap",
    "cdn"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cyf-super/vite-plugin-add-importmap",
    "directory": "src"
  },
  "dependencies": {
    "typescript": "^5.0.4",
    "vite": "^4.3.9"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "standard-version": "^9.5.0",
    "tsup": "^6.7.0"
  },
  "peerDependencies": {
    "vite": ">=2.5.0"
  }
}
